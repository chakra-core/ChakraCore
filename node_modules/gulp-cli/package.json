    "eslint-config-gulp": "^3.0.1",
    "expect": "^1.20.2",
    "gulp": "^4.0.0",
    "gulp-test-tools": "^0.6.1",
    "marked-man": "^0.2.1",
    "mocha": "^3.2.0",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.1"
