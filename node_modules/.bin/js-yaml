#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../generator-jhipster/node_modules/js-yaml/bin/js-yaml.js" "$@"
  ret=$?
else 
  node  "$basedir/../generator-jhipster/node_modules/js-yaml/bin/js-yaml.js" "$@"
  ret=$?
fi
exit $ret
