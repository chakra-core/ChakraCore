#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../babel-plugin-transform-es2015-unicode-regex/node_modules/regjsparser/bin/parser" "$@"
  ret=$?
else 
  node  "$basedir/../babel-plugin-transform-es2015-unicode-regex/node_modules/regjsparser/bin/parser" "$@"
  ret=$?
fi
exit $ret
