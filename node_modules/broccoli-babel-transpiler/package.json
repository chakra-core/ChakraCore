{
  "name": "broccoli-babel-transpiler",
  "version": "7.2.0",
  "description": "A Broccoli plugin which transpile ES6 to readable ES5 by using babel.",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "changelog": "lerna-changelog",
